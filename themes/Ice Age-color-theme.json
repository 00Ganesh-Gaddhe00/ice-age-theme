			"settings": {
				"foreground": "#909090"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#C0C0C0"
			}
		},
		{
			"name": "Function, Special Method",
